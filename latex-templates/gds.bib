  author =  "P. A. Gardner and Gareth D. Smith and Mark J.  Wheelhouse and Uri D. Zarfaty",
  title =  "Local {Hoare} reasoning about {DOM}",
  booktitle =  "Proc. of the Twenty-Seventh {ACM} {SIGMOD}-{SIGACT}-{SIGART} Symposium on Principles of Database Systems: {PODS}'08, Vancouver, {BC}, Canada, June 9--11, 2008",
  publisher =  "ACM Press",
